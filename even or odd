using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace even_or_odd
{
    class Program
    {
        static void Main(string[] args)
        {
            int no = 90;
            if (no % 2 == 0)
            {
                Console.WriteLine("no is even");
            }
            else
            {
                Console.WriteLine("no is odd");
            }
            Console.ReadLine();
        }
    }
}
